  "scripts": {
    "postinstall": "ngcc",
    "start": "ng serve",
    "clean": "rm -rf ./build ./dist ./.nyc_output ./coverage",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "test": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "test:debug": "ng test --browsers Chrome",
    "lint": "yarn lint:ts-build ",
    "lint:ts": "eslint \"**/*.ts\"",
    "lint:ts-build": "eslint \"**/*.ts\" -f json -o ./build/eslint.json",
    "lint:fix": "yarn lint:ts --fix",
    "merge:coverage": "node merge-coverage.js",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "e2e": "ng e2e",
    "e2e:debug": "ng e2e --watch --headless=false",
    "update-northfork": "node ./node_modules/@northfork/ng-schematics/northfork-update.js"
  }
